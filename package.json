{
  "name": "slash-create-docs-bot",
  "version": "1.0.0",
  "description": "A template for slash-create",
  "main": "dist/index.js",
  "scripts": {
    "format": "biome format --write ./src/*",
    "lint": "biome lint --apply ./src/*",
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "bun ./src/index.ts",
    "lint:fix": "npx eslint --ext .ts ./src --fix"
  },
  "dependencies": {
    "chrono-node": "^2.7.3",
    "convert-units": "^3.0.0-beta.6",
    "fuzzy": "^0.1.3",
    "slash-create": "^6.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
    "@types/bun": "^1.0.8",
    "slash-up": "^1.4.2",
    "ts-node": "^10.9.1",
    "typescript": "5.4.1-rc"
  }
}
